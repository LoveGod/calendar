#: calendar.module:92;162;1472;1484;1591;1720
#: calendar.info:0
msgid "Calendar"
msgstr "Kalender"

#: calendar.module:115
msgid "The Calendar View requires at least one field."
msgstr "Kalendervisningen kræver mindst ét felt."

#: calendar.module:123
msgid "You can only set on Default Sort on one field."
msgstr "Du kan kun vælge standardsortering på ét felt."

#: calendar.module:137
msgid "Calendar arguments must be set to 'Display All Values'."
msgstr "Kalender-argumenter skal sættes til 'Vis alle værdier'."

#: calendar.module:144
msgid "The Calendar requres as arguments Calendar: Year, Calendar: Month, and Calendar: Day, or Calendar: Year and Calendar: Week"
msgstr "Kalendervisningen kræver følgende argumenter: \"Kalender: År\", \"Kalender: Måned\" og \"Kalender: Dag\" eller \"Kalender: År\" og \"Kalender: Uge\"."

#: calendar.module:150
msgid "Calendar CCK Date fields must be set to 'Do not group multiple values'."
msgstr "Kalender CCK dato-felter skal sættes til 'gruppér ikke på forskellige værdier'."

#: calendar.module:916
msgid "Calendar: Year"
msgstr "Kalender: År"

#: calendar.module:918
msgid "Filter by the calendar year (YYYY)."
msgstr "Filtrér efter årstal (ÅÅÅÅ)."

#: calendar.module:921
msgid "Calendar: Month"
msgstr "Kalender: Måned"

#: calendar.module:923
msgid "Filter by the calendar month (1-12). Place this argument after a 'Year' argument."
msgstr "Filtrér efter måned (1-12). Placeres efter argumentet 'år'."

#: calendar.module:926
msgid "Calendar: Day"
msgstr "Kalender: Dag"

#: calendar.module:928
msgid "Filter by the calendar day (1-31). Place this argument after a 'Year' and a 'Month' argument."
msgstr "Filtrér efter kalenderdag (1-31). Placeres efter argumenterne 'år' og 'måned'."

#: calendar.module:931
msgid "Calendar: Week"
msgstr "Kalender: Uge"

#: calendar.module:933
msgid "Filter by the week number (1-52). Place this argument after a 'Year' argument and use a 'W' in front of the week number in the url."
msgstr "Filtrér efter ugenummer (1-52). Placeres efter argumentet 'år'. Indsæt et 'W' før ugenummeret i URLen."

